<h1>All repair orders</h1>

<table class="index-table">
  <tr>
    <th>ID</th>
    <th>Brand</th>
    <th>TAT</th>
    <th>Created</th>
    <th>Last update</th>
    <th>Next TAT</th>
  </tr>

  <% @repairs.each do |repair| %>
   <tr>
    <td>
        <%= link_to repair.id, repair %>
    </td>
    <td>
        <%= repair.brand %>
    </td>
    <td>
        N/A
    </td>
    <td>
        <%= repair.created_at %>
    </td>
    <td>
        <%= repair.updated_at %>
    </td>
    <td>
        N/A
    </td>
  </tr>
  <% end %>
</table>
